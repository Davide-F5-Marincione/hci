<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="navVM" type="com.mobiliteamdhiwisever.app.modules.nav.data.viewmodel.NavVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearNAV"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_200">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTOPBAR"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyleindigo_400">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSearchingRoute"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_weight="0.95"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{navVM.navModel.txtSearchingRoute}"
                        style="@style/txtRobotoromanbold24_1" tools:text="@string/msg_searching_route"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageSettings"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_111pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <TextView android:ellipsize="end" android:id="@+id/txtTo"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingStart="@dimen/_26pxh"
            android:paddingTop="@dimen/_12pxv" android:singleLine="true"
            android:text="@{navVM.navModel.txtTo}"
            style="@style/txtSolidOutline" tools:text="@string/lbl_from"/>
        <TextView android:ellipsize="end" android:id="@+id/txtToOne"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingStart="@dimen/_26pxh"
            android:paddingTop="@dimen/_12pxv" android:singleLine="true"
            android:text="@{navVM.navModel.txtToOne}"
            style="@style/txtSolidOutline" tools:text="@string/lbl_to"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtSUGGESTEDROUTE"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{navVM.navModel.txtSUGGESTEDROUTE}"
            style="@style/txtInterbold16" tools:text="@string/msg_suggested_route"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnlanguage"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_200">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSummarycard"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_463pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{navVM.navModel.txtLanguage}"
                    style="@style/txtRobotoregular20" tools:text="@string/lbl_12_30_12_45"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowqrcode"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearTransitIcon"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_weight="0.12"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:gravity="start"
                            android:id="@+id/imageQrcode"
                            android:layout_height="@dimen/_46pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_qrcode"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_qrcode"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMeasurement"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_4pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{navVM.navModel.txtMeasurement}"
                            style="@style/txtRobotoregular20" tools:text="@string/lbl_3l"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_29pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearTransitIcon1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_weight="0.42"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageCar"
                            android:layout_height="@dimen/_46pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_34pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_car"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_car"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{navVM.navModel.txtTwo}"
                            style="@style/txtRobotoregular20" tools:text="@string/lbl_310"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_21pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.39"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{navVM.navModel.txtTime}"
                        style="@style/txtRobotoregular36" tools:text="@string/lbl_15_min"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowtimeOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:id="@+id/txtTimeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.72"
                        android:layout_width="0dp"
                        android:text="@{navVM.navModel.txtTimeOne}"
                        style="@style/txtRobotoregular15" tools:text="@string/msg_delays_5_min"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSelect"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginStart="@dimen/_96pxh"
                        android:layout_width="@dimen/_70pxh"
                        android:paddingHorizontal="@dimen/_10pxh"
                        android:paddingVertical="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_select"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_2" tools:text="@string/lbl_select"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
